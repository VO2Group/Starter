{
  "name": "starter",
  "version": "1.0.0",
  "description": "Write smart hybrid apps",
  "scripts": {
    "start": "browser-sync start -s www -f '**/*'",
    "check": "jscs www/scripts",
    "copy": "npm run copy:ios && npm run copy:android",
    "copy:ios": "cp -R www/ platforms/ios/www",
    "copy:android": "cp -R www/ platforms/android/app/src/main/assets/www"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "jscs": "^3.0.7"
  }
}
